<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.b2infosoft.giftcardup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.Cart"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.Splash"
            android:theme="@style/AppFullScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Main"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Login"
            android:theme="@style/MyNoActionBar"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.SignUp"
            android:label="SignUp"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.ForgotPassword"
            android:label="ForgotPassword"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.Testexample" />
        <activity android:name=".activity.CompanyCard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity android:name=".activity.CompanyCard_1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity android:name=".activity.Categories_1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity android:name=".activity.Categories">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.ShoppingCart"
            android:label="Shopping Cart">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.MyProfile"
            android:label="PROFILE"
            android:theme="@style/MyNoActionBar"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.AddAccountInfo"
            android:label="AddAccount"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.AddIdentity"
            android:label="Identity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.AddSSN"
            android:label="SSN"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.UpdateAccountInfo"
            android:label="UpdateAccount"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.EditGiftCard"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.PaymentWithdrawalRequest"
            android:label="PaymentWithdrawalRequest"
            android:windowSoftInputMode="adjustResize" />

        <receiver
            android:name=".recevier.MyReceiver"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name=".services.CartStatus"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".activity.CheckOut"
            android:label="Checkout">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.ShoppingCart" />
        </activity>
        <activity
            android:name=".activity.ChangeAddress"
            android:label="Change Address"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.Payments"
            android:label="Payment">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.CheckOut" />
        </activity>
        <activity
            android:name=".activity.PayAvailableFun"
            android:label="Payment">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Payments" />
        </activity>
        <activity
            android:name=".activity.PayCreditDebit"
            android:label="Payment">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Payments" />
        </activity>
        <activity
            android:name=".activity.PayPayPal"
            android:label="Payment">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Payments" />
        </activity>
        <activity
            android:name=".activity.PlaceOrder"
            android:label="Place Order" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".activity.FBUserUpdate"
            android:label="Profile Update"
            android:windowSoftInputMode="adjustResize">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.ProfileNew"
            android:label="@string/title_activity_profile_new"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
        <activity
            android:name=".activity.ProfileIdentification"
            android:label="Identification"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.ProfileBankDetails"
            android:label="Bank Details"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.ProfileSsnEin"
            android:label="SSN/EIN"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.AddAccountInfo1" />
        <activity android:name=".activity.UpdateAccountInfo1" />
        <activity
            android:name=".activity.NotificationActivity"
            android:label="Notifications"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.Main" />
        </activity>
    </application>
</manifest>
